{
  "root": true,
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "plugin:node/recommended"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es6",
        "endOfLines": "LF"
      }
    ],
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
