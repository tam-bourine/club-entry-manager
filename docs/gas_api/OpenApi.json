{
  "openapi": "3.0.2",
  "info": {
    "title": "GAS API",
    "version": "1.0",
    "description": "GAS API"
  },
  "tags": [
    {
      "name": "clubs"
    }
  ],
  "paths": {
    "?action=get": {
      "get": {
        "tags": ["clubs"],
        "summary": "入部申請用 部活動一覧",
        "description": "入部可能な部活動一覧の取得",
        "parameters": [
          {
            "in": "query",
            "name": "action",
            "schema": {
              "type": "string"
            },
            "description": "GASでルーティング分けに使用"
          }
        ],
        "responses": {
          "201": {
            "description": "成功時のレスポンス",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "message": {
                      "type": "string",
                      "example": "200 OK"
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "clubs": {
                      "type": "array",
                      "example": [
                        {
                          "id": "f00tsa1",
                          "name": "フットサル部"
                        },
                        {
                          "id": "ho1ge2fu3ga4",
                          "name": "ほげ部"
                        }
                      ]
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "失敗時のレスポンス",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 500
                    },
                    "message": {
                      "type": "string",
                      "example": "500 Internal Server Error"
                    },
                    "success": {
                      "type": "boolean",
                      "example": false
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "?action=regist": {
      "post": {
        "tags": ["club"],
        "summary": "創部申請",
        "description": "部活動シートの末尾に申請のあった部活を追加します。",
        "parameters": [
          {
            "in": "query",
            "name": "action",
            "schema": {
              "type": "string"
            },
            "description": "GASでルーティング分けに使用"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "club": {
                    "type": "object",
                    "example": {
                      "name": "テニス部",
                      "description": "〜〜する",
                      "budgetUse": "テニス用具やドリンクの購入",
                      "kibelaUrl": "https://kibe.la/",
                      "channelId": "C02DAE19"
                    }
                  },
                  "captain": {
                    "type": "object",
                    "example": {
                      "slackId": "F2FDKWOI",
                      "name": "キャプテンキッド"
                    }
                  },
                  "members": {
                    "type": "array",
                    "example": [
                      {
                        "slackId": "XEF8FDSX",
                        "name": "ほげ山ほげ子"
                      },
                      {
                        "slackId": "F39SFDW",
                        "name": "ふが田ふが男"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "成功時のレスポンス",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 201
                    },
                    "message": {
                      "type": "string",
                      "example": "201 Created"
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "club": {
                      "type": "object",
                      "example": {
                        "id": "fugapiyo",
                        "name": "テニス部"
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "失敗時のレスポンス",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 500
                    },
                    "message": {
                      "type": "string",
                      "example": "500 Internal Server Error"
                    },
                    "success": {
                      "type": "boolean",
                      "example": false
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "?action=approve": {
      "post": {
        "summary": "創部承認",
        "description": "該当部活動の認可をTRUEにして、新しく部活動タブを作成する",
        "tags": ["club"],
        "parameters": [
          {
            "in": "query",
            "name": "action",
            "schema": {
              "type": "string"
            },
            "description": "GASでルーティング分けに使用"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "slackChannelId": {
                    "type": "string",
                    "example": "fugapiyo"
                  },
                  "authorizer": {
                    "type": "object",
                    "example": {
                      "slackId": "19DPICZZ",
                      "name": "ピヨ橋ピヨ美"
                    }
                  },
                  "isApproved": {
                    "type": "boolean"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "承認成功時のレスポンス",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 201
                    },
                    "message": {
                      "type": "string",
                      "example": "201 Created"
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    }
                  }
                }
              }
            }
          },
          "200": {
            "description": "却下成功時のレスポンス",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "message": {
                      "type": "string",
                      "example": "200 OK"
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "失敗時のレスポンス",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 500
                    },
                    "message": {
                      "type": "string",
                      "example": "500 Internal Server Error"
                    },
                    "success": {
                      "type": "boolean",
                      "example": false
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "?action=join": {
      "post": {
        "summary": "入部申請",
        "description": "申請のあった該当部活動シートに、申請者の名前を追加",
        "tags": ["club"],
        "parameters": [
          {
            "in": "query",
            "name": "action",
            "schema": {
              "type": "string"
            },
            "description": "GASでルーティング分けに使用"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "slackChannelId": {
                    "type": "string",
                    "example": "fugapiyo"
                  },
                  "member": {
                    "type": "object",
                    "example": {
                      "slackId": "DFKEIOD",
                      "name": "風本颯太"
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "成功時のレスポンス",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 201
                    },
                    "message": {
                      "type": "string",
                      "example": "201 Created"
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "失敗時のレスポンス",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 500
                    },
                    "message": {
                      "type": "string",
                      "example": "500 Internal Server Error"
                    },
                    "success": {
                      "type": "boolean",
                      "example": false
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
