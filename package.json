{
  "name": "club-manager",
  "version": "1.0.0",
  "description": "社内の部活管理を Slack で",
  "main": "lib/index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "lint": "eslint --ext \"./src/**/*.ts\" \"./**/*.json\" --cache",
    "lint:fix": "eslint --fix --ext \"./src/**/*.ts\" \"./**/*.json\" --cache",
    "firebase:serve": "firebase emulators:start --only functions",
    "firebase:shell": "firebase functions:shell",
    "firebase:start": "npm run shell",
    "firebase:deploy": "firebase deploy --only functions",
    "firebase:logs": "firebase functions:log",
    "postinstall:types-gas": "npx replace-in-file \"declare var console\" \"//declare var console\" node_modules/@types/google-apps-script/google-apps-script.base.d.ts",
    "serve": "tsc -w && npx node lib/functions/index.js"
  },
  "keywords": [
    "GAS",
    "clasp",
    "Spreadsheet",
    "Club",
    "Slack",
    "Bolt",
    "Kibela",
    "Node",
    "TypeScript",
    "Firebase Functions",
    "社内部活管理"
  ],
  "author": "tam-bourine",
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^3.2.0",
    "@slack/web-api": "^6.0.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/google-apps-script": "^1.0.20",
    "@types/graphql": "^14.5.0",
    "@types/node": ">=10",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-googleappsscript": "^1.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "firebase-functions-test": "^0.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
