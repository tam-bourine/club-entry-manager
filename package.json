{
  "name": "club-manager",
  "version": "1.0.0",
  "description": "社内の部活管理を Slack で",
  "main": "lib/index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "tsc": "./node_modules/.bin/tsc",
    "lint": "eslint src --ext .ts,.json --cache",
    "lint --no-cache": "eslint src --ext .ts,.json",
    "lint:fix": "eslint src --fix --ext .ts,.json --cache",
    "lint:fix --no-cache": "eslint src --fix --ext .ts,.json",
    "run": "node lib/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "postinstall:types-gas": "npx replace-in-file \"declare var console\" \"//declare var console\" node_modules/@types/google-apps-script/google-apps-script.base.d.ts",
    "start": "npm ci && npm run postinstall:types-gas && npm run bolt:serve",
    "bolt:serve": "npm run tsc && npx node lib/index.js",
    "bolt:dev": "npm run tsc && node --require dotenv/config --inspect=localhost:9229 lib/index.js",
    "bolt:dev:watch": "ts-node-dev --transpile-only src/index.ts",
    "codegen": "graphql-codegen"
  },
  "keywords": [
    "GAS",
    "clasp",
    "Spreadsheet",
    "Club",
    "Slack",
    "Bolt",
    "Kibela",
    "Node",
    "TypeScript",
    "Google Cloud Run",
    "Cloud Run",
    "GCR",
    "tambourine",
    "社内部活管理"
  ],
  "author": "tam-bourine",
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^3.2.0",
    "@slack/web-api": "^6.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/import-types-preset": "^1.18.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@types/google-apps-script": "^1.0.20",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.21",
    "@types/node": ">=10",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-googleappsscript": "^1.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
